{
  "header": "# Changelog\n\n",
  "types": [
    {"type": "feat", "section": "## Features"},
    {"type": "fix", "section": "## Bug Fixes"}
  ],
  "test": false,
  "host": null,
  "owner": null,
  "repository": null,
  "commitUrlFormat": "{{@root.host}}/{{@root.owner}}/{{@root.repository}}/commit/{{hash}}",
  "compareUrlFormat": "{{@root.host}}/{{@root.owner}}/{{@root.repository}}/compare/{{previousTag}}...{{currentTag}}",
  "issueUrlFormat": "{{@root.host}}/{{@root.owner}}/{{@root.repository}}/issues/{{issue}}",
  "userUrlFormat": "{{host}}/{{user}}",
  "releaseCommitMessageFormat": "chore(release): {{currentTag}}",
  "releaseNotesFormat": "- {{text}}",
  "changelogFilename": "CHANGELOG.md",
  "commitMessageFormat": "{{type}}({{scope}}): {{subject}}",
  "tagPrefix": "",
  "tagFormat": "{{version}}",
  "initialVersion": "0.1.0",
  "headerPattern": "^# Changelog$",
  "releaseCommitMessage": "chore(release): {{currentTag}}",
  "compareUrl": "{{@root.host}}/{{@root.owner}}/{{@root.repository}}/compare/{{previousTag}}...{{currentTag}}",
  "releaseAs": "major",
  "skip": {
    "commit": false,
    "tag": false
  },
  "format": "## [{{version}}]({{compareUrl}}) ({{date format='YYYY-MM-DD'}})"
}
