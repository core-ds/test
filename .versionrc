{
  "header": "# Changelog\n\n",
  "types": [
    {"type": "feat", "section": "## Features"},
    {"type": "fix", "section": "## Bug Fixes"}
  ],
  "commitUrlFormat": "{{host}}/{{owner}}/{{repository}}/commit/{{hash}}",
  "compareUrlFormat": "{{host}}/{{owner}}/{{repository}}/compare/{{previousTag}}...{{currentTag}}",
  "issueUrlFormat": "{{host}}/{{owner}}/{{repository}}/issues/{{id}}",
  "userUrlFormat": "{{host}}/{{user}}",
  "releaseCommitMessageFormat": "{{currentTag}}",
  "releaseNotesFormat": "- {{text}}",
  "changelogFilename": "CHANGELOG.md",
  "commitMessageFormat": "{{type}}({{scope}}): {{subject}}",
  "tagPrefix": "",
  "tagFormat": "{{version}}",
  "initialVersion": "0.1.0",
  "headerPattern": "^# Changelog$",
  "releaseCommitMessage": "chore(release): {{currentTag}}",
  "releaseAs": "major",
  "skip": {
    "commit": false,
    "tag": false
  },
  "format": "## [{{version}}]({{compareUrl}}) ({{date format='YYYY-MM-DD'}})\n\n{{#each commits}}{{#with footer}}{{#if BREAKING CHANGE}}- **{{BREAKING CHANGE}}**\n{{/if}}{{/with}}{{/each}}",
  "test": false,
  "host": "https://github.com",
  "owner": "owner",
  "repository": "repository"
}
