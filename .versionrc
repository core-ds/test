{
  "header": "## Changelog\n\n",
  "types": [
    {"type": "feat", "section": "Features"},
    {"type": "fix", "section": "Bug Fixes"},
    {"type": "docs", "section": "Documentation Changes"},
    {"type": "style", "section": "Styles"},
    {"type": "refactor", "section": "Code Refactoring"},
    {"type": "perf", "section": "Performance Improvements"},
    {"type": "test", "section": "Tests"},
    {"type": "build", "section": "Build System"},
    {"type": "ci", "section": "Continuous Integration"},
    {"type": "chore", "section": "Chores"},
    {"type": "revert", "section": "Reverts"}
  ],
  "commitUrlFormat": "{{host}}/{{owner}}/{{repository}}/commit/{{hash}}",
  "compareUrlFormat": "{{host}}/{{owner}}/{{repository}}/compare/{{previousTag}}...{{currentTag}}",
  "issueUrlFormat": "{{host}}/{{owner}}/{{repository}}/issues/{{id}}",
  "userUrlFormat": "{{host}}/{{user}}",
  "releaseCommitMessageFormat": "{{currentTag}}",
  "releaseNotesFormat": "- {{text}}",
  "changelogFilename": "CHANGELOG.md",
  "commitMessageFormat": "{{type}}({{scope}}): {{subject}}",
  "tagPrefix": "",
  "tagFormat": "{{version}}",
  "initialVersion": "0.1.0",
  "headerPattern": "^## Changelog$",
  "releaseCommitMessage": "chore(release): {{currentTag}}",
  "releaseAs": "major",
  "skip": {
    "commit": false,
    "tag": false
  },
  "format": "{{#if tag}}{{#if commits}}### {{tag}} ({{date format='YYYY-MM-DD'}})\n\n{{#each commits}}{{#with footer}}{{#if BREAKING CHANGE}}- **{{BREAKING CHANGE}}**\n{{/if}}{{/with}}{{/each}}{{else}}## [Unreleased]{{/if}}{{else}}### [{{initialVersion}}]{{/if}}",
  "test": false
}
